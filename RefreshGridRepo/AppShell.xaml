<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="RefreshGridRepo.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:RefreshGridRepo"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent Title="Home" ContentTemplate="{DataTemplate local:MainPage}" Route="MainPage" />

    <ShellContent Shell.FlyoutItemIsVisible="False" ContentTemplate="{DataTemplate local:MainPage1}" Route="MainPage1"/>
    <ShellContent Shell.FlyoutItemIsVisible="False" ContentTemplate="{DataTemplate local:MainPage2}" Route="MainPage2"/>
    <ShellContent Shell.FlyoutItemIsVisible="False" ContentTemplate="{DataTemplate local:MainPage3}" Route="MainPage3"/>
    <ShellContent Shell.FlyoutItemIsVisible="False" ContentTemplate="{DataTemplate local:MainPage4}" Route="MainPage4"/>
    <ShellContent Shell.FlyoutItemIsVisible="False" ContentTemplate="{DataTemplate local:Icons}" Route="Icons"/>
    
</Shell>
